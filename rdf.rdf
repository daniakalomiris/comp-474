<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:p="http://example.org/"
    xmlns:pdata="http://example.org/data#/"
    xmlns:wd="http://www.wikidata.org/entity/"
    xmlns:db="http://www.dbpedia.org/resource/"
    xmlns:schema="http://schema.org/"
    xmlns:el="http://example.org/elements/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#" >
  <rdf:Description rdf:about="http://example.org/University">
     <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
     <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
     <rdfs:label xml:lang="en">University</rdfs:label>
     <rdfs:comment>Organization at which the students go to</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/Course">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">Course</rdfs:label>
    <rdfs:comment>Course is offered at a university towards the granting of an approved degree</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/Topic">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">Topic</rdfs:label>
    <rdfs:comment>Topic is part of a course material</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/Student">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/person"/>
    <rdfs:label xml:lang="en">Course</rdfs:label>
    <rdfs:comment>Person who studies at a university</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/hasTopic">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">hasTopic</rdfs:label>
    <rdfs:comment>Course has a topic</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/Course"/>
    <rdfs:range rdf:resource="http://example.org/Topic"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/hasCompleted">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">hasCompleted</rdfs:label>
    <rdfs:comment>Student has completed a course</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/Student"/>
    <rdfs:range rdf:resource="http://example.org/Course"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/hasGrade">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:subPropertyOf rdf:resource="http://example.org/hasCompleted"/>
    <rdfs:label xml:lang="en">hasGrade</rdfs:label>
    <rdfs:comment>Student has a grade for a completed course</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/hasCompleted"/>
    <rdfs:range xsd:string/>
  </rdf:Description>
</rdf:RDF>